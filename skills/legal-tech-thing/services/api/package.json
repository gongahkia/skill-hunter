    "prisma:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.917.0",
    "@aws-sdk/s3-request-presigner": "^3.917.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@prisma/client": "^6.16.3",
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.63.2",
    "cheerio": "^1.1.2",
    "fastify": "^5.6.1",
    "fastify-plugin": "^5.1.0",
    "ioredis": "^5.7.0",
    "jose": "^6.1.2",
    "zod": "^3.24.1",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/zxcvbn": "^4.4.5",
    "prisma": "^6.16.3",
    "tsx": "^4.20.6"
  }
